{
  "version": 2,
  "name": "fractal-bot-web",
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/web/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
